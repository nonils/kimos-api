{
  "family": "kimos-api",
  "networkMode": "bridge",
  "containerDefinitions": [
    {
      "name": "kimos-api",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 10101,
          "hostPort": 80
        }
      ],
      "memory": 1024,
      "environment": [
      ],
      "links": [
        "redis"
      ]
    },
    {
      "name": "redis",
      "image": "redis:latest",
      "essential": true,
      "memory": 1024,
      "portMappings": [
        {
          "containerPort": 6379,
          "hostPort": 10100
        }
      ]
    }
  ]
}